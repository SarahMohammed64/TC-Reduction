      "source": [
        "\n",
        "def calculate_occurrences(items):\n",
        "    occurrences = {}\n",
        "    for item in items:\n",
        "        c = items.count(item)\n",
        "        occurrences[item] = c\n",
        "    return occurrences\n",
        "\n",
        "items = ['apple', 'banana', 'orange', 'apple', 'banana', 'apple']\n",
        "result = calculate_occurrences(items)\n",
        "print(result)"
